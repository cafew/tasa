<h1 class='page-header'>
  Created by
  <%= image_tag 'pivotal.png', class: 'logo pivotal-logo-header', alt: 'Pivotal' %>
  Data Labs and
  <%= image_tag 'pivotal_labs.png', class: 'logo pivotal-labs-logo-header', alt: 'Pivotal Labs' %>
</h1>

<form class="query" onsubmit="event.preventDefault()">
  <input class="query-input" name="query" placeholder="Type to search" autocomplete="off">
  <span class="de-emphasize help-text">Press enter when done</span>
  <span class="de-emphasize info-text">Tweets from June 30 - July 31 of 2013</span>
</form>

<section class="graphs">
  <article class="total-tweets">
    <div class="graph-title">Total tweets</div>
    <div class="y-axis-left"></div>
    <div class="y-axis-right"></div>
    <div class="graph"></div>
    <div class="x-axis"></div>
  </article>
</section>